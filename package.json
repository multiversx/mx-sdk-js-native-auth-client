{
  "name": "@multiversx/sdk-native-auth-client",
  "version": "1.0.10-alpha.6",
  "description": "Native authentication client-side",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "test": "jest test/*",
    "compile": "tsc && tsc-alias",
    "build": "node esbuild.js && tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "unpublish-verdaccio": "npm unpublish @multiversx/sdk-native-auth-client --force --registry http://localhost:4873",
    "publish-verdaccio": "npm run unpublish-verdaccio && npm run build && npm publish --registry http://localhost:4873/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MultiversX/mx-sdk-erdjs-native-auth-client.git"
  },
  "keywords": [
    "multiversx",
    "native",
    "auth",
    "client"
  ],
  "author": "MultiversX",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "esbuild": "0.21.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.16.0",
    "axios-mock-adapter": "^1.20.0",
    "eslint": "^8.9.0",
    "jest": "^29.7.0",
    "typescript": "^4.3.5",
    "tsc-alias": "1.8.9"
  },
  "dependencies": {
    "axios": "^1.7.4"
  }
}
